{
  "manifest_version": 2,
  "name": "Chrome X",
  "description": "Show history",
  "version": "1.0",
  "chrome_url_overrides":{
    "history" : "history.html"
  },
  "browser_action": {
    "default_title": "History"
  },
  "permissions": [
    "history",
    "chrome://favicon/"
  ],
  "icons": {
    "16":"init.PNG"
  }
}
