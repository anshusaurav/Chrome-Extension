{
  "manifest_version": 2,
  "name": "Chrome X",
  "short_name":"chromeX",
  "description": "The one of the best cleaning tools for your Chromebook, Windows PC, Mac or Linux. https://github.com/anshusaurav/Chrome-Extension",
  "version": "2.0",
  "chrome_url_overrides":{
    "history" : "history.html"
  },
  "browser_action": {
    "default_title": "History"
  },
  "permissions": [
    "history",
    "chrome://favicon/"
  ],
  "icons": {
    "16":"li16X16.PNG",
    "32": "li32X32.PNG",
    "48": "li64X64.PNG",
    "128": "li128X128.PNG"
  }
}
